    "test-migrate": "cross-env NODE_ENV=test npx sequelize db:migrate ",
    "test-seed": "cross-env NODE_ENV=test npx sequelize db:seed:all",
    "post-test": "cross-env NODE_ENV=test npx sequelize db:drop",
    "dev": "nodemon index.js",
    "start": "node index.js"
